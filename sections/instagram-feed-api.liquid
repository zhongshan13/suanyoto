<style>.hdt-ins-loading,.hdt-ins-loading:after{border-radius:var(--rounded-full);width:5rem;height:5rem}.hdt-ins-loading{margin:0 auto;position:relative;border-top:.3rem solid rgb(var(--color-accent)/.2);border-right:.3rem solid rgb(var(--color-accent)/.2);border-bottom:.3rem solid rgb(var(--color-accent)/.2);border-left:.3rem solid rgb(var(--color-accent));transform:translateZ(0);-webkit-animation:ins_load 8.1s infinite linear;animation:ins_load 1.5s infinite linear}@-webkit-keyframes ins_load{0%{-webkit-transform:rotate(0deg);transform:rotate(0deg)}100%{-webkit-transform:rotate(360deg);transform:rotate(360deg)}}@keyframes ins_load{0%{-webkit-transform:rotate(0deg);transform:rotate(0deg)}100%{-webkit-transform:rotate(360deg);transform:rotate(360deg)}}.hdt-ins-inner .hdt-ins-img{transition:all 0.3s ease}.hdt-ins-icon{position:absolute;z-index:5;top:50%;opacity:0;left:50%;transform:translate(-50%,-50%);transition:.5s}.hdt-ins-icon svg{width:2.4rem;height:2.4rem;fill:#ededed}@media (-moz-touch-enabled:0),(hover:hover) and (min-width:1150px){.hdt-ins-inner:hover .hdt-ins-img{transform:scale(1.05);filter:brightness(.8)}.hdt-ins-inner:hover .hdt-ins-icon{opacity:1}}</style>{%- style -%}#shopify-section-{{section.id}}{--section-pt:{{section.settings.padding_top}}px;--section-pb:{{section.settings.padding_bottom}}px;--section-prate:{{section.settings.padding_rate}}}{%- endstyle -%}
