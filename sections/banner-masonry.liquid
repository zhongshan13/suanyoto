        {%- endcase -%}
      {% endfor %}
    </div>
  </div>
</div>
{% schema %}
{
  "name": "t:sections.banner.name_masonry",
  "tag": "section",
  "class": "hdt-banner hdt-section section-allow-transparent",
  "disabled_on": {
    "groups": [
      "*"
    ]
  },
  "settings": [
    {
      "type": "color_scheme",
      "id": "color_scheme",
      "label": "t:sections.banner.blocks.common.color"
    },
    {
      "type": "header",
      "content": "t:sections.banner.settings.desktop"
    },
    {
      "type": "range",
      "id": "row_height",
      "min": 100,
      "max": 500,
      "step": 5,
      "unit": "px",
      "label": "t:sections.banner.settings.row_height",
      "default": 200
    },
    {
      "type": "header",
      "content": "t:sections.banner.settings.mobile"
    },
    {
      "type": "range",
      "id": "row_height_mb",
      "min": 100,
      "max": 400,
      "step": 5,
      "unit": "px",
      "label": "t:sections.banner.settings.row_height",
      "default": 150
    },
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 40
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 40
    },
    {
      "type": "range",
      "id": "padding_rate",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "%",
      "label": "t:sections.all.padding.padding_rate",
      "default": 75
    }
  ],
  "blocks": [
    {
      "type": "banner_1",
      "name": "t:sections.banner.blocks.banner_1.label",
      "settings": [
        {
          "type": "image_picker",
          "id": "image",
          "label": "t:sections.banner.blocks.common.image"
        },
        {
          "type": "image_picker",
          "id": "image_mb",
          "label": "t:sections.banner.blocks.common.image_mb"
        },
        {
          "type": "color_scheme",
          "id": "color_scheme",
          "label": "t:sections.banner.blocks.common.color"
        },
        {
          "type": "url",
          "id": "banner_url",
          "label": "t:sections.banner.blocks.common.banner_url"
        },
        {
          "type": "checkbox",
          "id": "banner_target",
          "label": "t:sections.all.button.settings.target.label",
          "default": false
        },
        {
          "type": "textarea",
          "id": "sub_heading",
          "label": "t:sections.all.sub_heading.label",
          "default": "Every piece is made to last beyond the season"
        },
        {
          "type": "select",
          "id": "sub_heading_fs",
          "label": "t:sections.all.sub_heading.sub_f_s.label",
          "options": [
            {
              "value": "h3",
              "label": "t:sections.all.sub_heading.sub_f_s.options.h3"
            },
            {
              "value": "h4",
              "label": "t:sections.all.sub_heading.sub_f_s.options.h4"
            },
            {
              "value": "h5",
              "label": "t:sections.all.sub_heading.sub_f_s.options.h5"
            },
            {
              "value": "h6",
              "label": "t:sections.all.sub_heading.sub_f_s.options.h6"
            },
            {
              "value": "base",
              "label": "t:sections.all.sub_heading.sub_f_s.options.base"
            }
          ],
          "default": "base"
        },
        {
          "type": "select",
          "id": "sub_heading_fw",
          "label": "t:sections.all.sub_heading.sub_f_w.label",
          "options": [
            {
              "value": "light",
              "label": "t:sections.all.sub_heading.sub_f_w.options.light"
            },
            {
              "value": "normal",
              "label": "t:sections.all.sub_heading.sub_f_w.options.normal"
            },
            {
              "value": "medium",
              "label": "t:sections.all.sub_heading.sub_f_w.options.medium"
            },
            {
              "value": "semibold",
              "label": "t:sections.all.sub_heading.sub_f_w.options.semibold"
            },
